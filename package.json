{
  "name": "dorf",
  "version": "2.0.0-beta.2",
  "description": "Domain Object Reactive Forms for Angular and TypeScript",
  "keywords": [
    "angular",
    "angular2",
    "angular4",
    "typescript",
    "ts",
    "reactive",
    "dynamic",
    "model-driven",
    "form",
    "forms",
    "html",
    "domain",
    "object",
    "decorator",
    "annotation",
    "decorators",
    "annotations"
  ],
  "scripts": {
    "patch": "npm run build && npm test && npm version patch",
    "test": "karma start config/karma/conf.js",
    "build": "tsc",
    "build-all": "npm run build && npm run build-examples",
    "build-examples": "npm run build-old-examples && npm run build-new-examples",
    "build-old-examples": "npm run simple-example-old && npm run field-example && npm run extras-example && npm run definition-example",
    "simple-example-old": "copyfiles ./src/**/*.js ./src/**/*.html ./src/**/*.d.ts ./examples/simple/old-style/node_modules/dorf/",
    "field-example": "copyfiles ./src/**/*.js ./src/**/*.html ./src/**/*.d.ts ./examples/extensions/new-field/node_modules/dorf/",
    "extras-example": "copyfiles ./src/**/*.js ./src/**/*.html ./src/**/*.d.ts ./examples/extensions/definition-extras/node_modules/dorf/",
    "definition-example": "copyfiles ./src/**/*.js ./src/**/*.html ./src/**/*.d.ts ./examples/extensions/different-definition/node_modules/dorf/",
    "build-new-examples": "npm run simple-example-new",
    "simple-example-new": "copyfiles ./src/**/*.js ./src/**/*.html ./src/**/*.d.ts ./examples/simple/new-style/node_modules/dorf/",
    "build-webpack": "webpack --progress --profile --bail --config config/webpack/lib.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mat3e/dorf"
  },
  "author": "Mateusz Chrzonstowski",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mat3e/dorf/issues"
  },
  "homepage": "https://mat3e.github.io/dorf/",
  "main": "index.js",
  "module": "index.js",
  "typings": "index.d.ts",
  "devDependencies": {
    "@angular/common": "~2.4.4",
    "@angular/compiler": "~2.4.4",
    "@angular/core": "~2.4.4",
    "@angular/forms": "~2.4.4",
    "@angular/platform-browser": "~2.4.4",
    "@angular/platform-browser-dynamic": "~2.4.4",
    "@types/core-js": "^0.9.34",
    "@types/node": "^6.0.45",
    "@types/jasmine": "^2.5.35",
    "@types/selenium-webdriver": "^2.53.33",
    "angular2-template-loader": "^0.6.0",
    "copyfiles": "1.0.0",
    "core-js": "^2.4.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "^1.8.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "raw-loader": "^0.5.1",
    "reflect-metadata": "^0.1.9",
    "rxjs": "5.0.1",
    "style-loader": "^0.13.1",
    "ts-loader": "^1.0.0",
    "typescript": "^2.0.9",
    "webpack": "^1.13.0",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.0",
    "zone.js": "^0.7.2"
  }
}